<ul class='list-post'>
    {{#recents}}
        <li>
            <h2><a href='/{{ post.category }}{{ post.loc }}'>{{ post.title }}</a></h2>
            <p><small><a href='/{{ post.category }}'>{{ post.category }}</a><span class="divider"></span>  {{ post.date }}</small></p>
            <div class='extract'>
                <p>{{{ post.extract }}}</p>
            </div>
            <hr>
        </li>
    {{/recents}}
</ul>